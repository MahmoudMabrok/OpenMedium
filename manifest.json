{
  "name": "Open Medium",
  "version": "1.1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["*://*.medium.com/*"],
      "js": ["content.js"]
    }
  ],
    
    "icons":{
        "128":"icon128.png", 
         "64":"icon64.png", 
         "16":"icon16.png"
    } , 

  "permissions": ["tabs"]
}